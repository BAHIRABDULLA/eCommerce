                                                            <%= userAddress.address[0].landmark %>
                                                                <%= userAddress.address[0].phone2 %><br>

                                    </div>
                                    <button class="btn-primary rounded-pill" data-toggle="modal"
                                        data-target="#addressModal">Change</button>
                                <% } else { %>
                                    <p>No Shipping address found</p>
                                <% } %>
