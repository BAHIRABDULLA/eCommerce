    <script>
        let selectedAddressId
        function selectAddress(addressId, userAddress) {
            console.log('Selected address:', addressId);

            selectedAddressId=addressId

            const selectedAddressElement = document.getElementById('selectedAddress');
            selectedAddressElement.innerHTML = '';
            const selectedAddress = userAddress.address.find(address => address._id === addressId);
            renderSelectedAddress(selectedAddress);
            $('#addressModal').modal('hide');
        }

        function renderSelectedAddress(address) {
            const selectedAddressElement = document.getElementById('selectedAddress');
            selectedAddressElement.innerHTML = `
                <strong>${address.name} &nbsp; ${address.phone}</strong><br>
                ${address.streetAddress}<br>
                ${address.city}, ${address.state} ${address.pincode}<br>
                ${address.email}<br>
                ${address.landmark}<br>
                ${address.phone2}<br>
            `;
        }


        document.getElementById('placeOrderBtn').addEventListener('click', async () => {

            try {

            const paymentMethod = document.querySelector('input[name="paymentMethod"]:checked').id;
            const totalAmount = parseFloat(document.querySelector('.summary-total td:last-child').innerText.replace('₹', '').trim());
            // const selectedAddressId=document.getElementById('selectedAddress').dataset.addressId;

            // const selectedAddressId = document.getElementById('selectedAddress').innerText.trim().split('\n')[0];
           
      
          
            console.log(selectedAddressId, 'itss selecdt address id ');
            const orderData = {
                userId: '<%= user_id %>',
                totalAmount,
                orderUserDetails:selectedAddressId,
                paymentMethod: 'Cash on Delivery',
            };

            
                const response = await fetch('/placeOrder', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json'
                    },
                    body: JSON.stringify(orderData)
                });
                if (response.ok) {
                    Swal.fire({
                        title: 'Order Placed Successfully',
                        icon: 'success'
                    });
                } else {
                    throw new Error('Failed to place order');
                }
            } catch (error) {
                console.error('Error placing order', error);
                Swal.fire({
                    title: 'Failed to Place Order',
                    text: 'Please try again later',
                    icon: 'error'
                });
            }
        });



    </script>


