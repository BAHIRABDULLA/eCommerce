        document.addEventListener('DOMContentLoaded', function () {
            const categoryRadios = document.querySelectorAll('input[name="category"]');
            const all = document.getElementById('cat-8');
            all.addEventListener('click', () => {
                console.log('allproduts ')
                window.location.href = `/products?category=allCateogry`
            })
            categoryRadios.forEach(radio => {
                console.log('fillersldsajfkalsdfjlsadkfjlsad;')
                radio.addEventListener('change', function () {
                    const categoryId = this.value
                    window.location.href = `/products?category=${categoryId}`
                })
            })
        })

        function updateProdcutList(products) {
            const productsContainer = document.querySelector('.products.row');
            productsContainer.innerHTML = ''
            products.forEach(product => {
                const productHTML = `
                <div class="col-6 col-md-4 col-lg-4 col-xl-3">
                
                `;
                productsContainer.insertAdjacentHTML('beforend', productHTML)
            })
        }


        // this is for searching option

        document.addEventListener('DOMContentLoaded', function () {
            const searchForm = document.getElementById('searchForm');
            searchForm.addEventListener('submit', function (event) {
                event.preventDefault(); // Prevent the form from submitting normally

                const searchInput = document.getElementById('q');
                const searchQuery = searchInput.value.trim();

                if (searchQuery) {
                    window.location.href = `/search?q=${encodeURIComponent(searchQuery)}`;
                }
            });
        });

