                            const pricePerUnit = parseFloat($('#price_' + productId).text().replace('â‚¹', ''))
                            const quantity = parseInt($('#quantity_' + productId).val())
                            const totalPrice = pricePerUnit * quantity;
                            const subtotalElement = $('.summary-subtotal td:last-child');
                            const subtotal = parseFloat(subtotalElement.text().replace('â‚¹', ''))
                            const newSubtotal = subtotal - totalPrice
                            productRow.remove();
                            console.log('Item removed successfully');
                            Swal.fire(
                                'Deleted!',
                                'Your item has been removed from cart.',
                                'success'
                            );
                            subtotalElement.text('â‚¹'+newSubtotal.toFixed(2))
                            calculateTotal()
                          
