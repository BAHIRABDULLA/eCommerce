        }


        function calculateTotal() {
            let subtotal = 0
            $('.total-price').each(function () {
                subtotal += parseFloat($(this).text().replace('â‚¹', ''))
            })
            let discount = 0
            let deliveryCharges = subtotal < 500 ? 50 : 0
            let totalAmount = subtotal - discount + deliveryCharges

            $('.summary-subtotal td:last-child').text('â‚¹' + subtotal.toFixed(2))
            $('.summary-discount td:last-child').text('â‚¹' + discount.toFixed(2))
            $('.summary-delivery td:last-child').text('â‚¹' + deliveryCharges.toFixed(2))
            $('.summary-total td:last-child').text('â‚¹' + totalAmount.toFixed(2))
        }

        calculateTotal();

        $('.quantity-input').on('change', function () {
            updateTotalPrice(this);
            calculateTotal()
