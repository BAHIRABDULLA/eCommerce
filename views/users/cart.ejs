                                                        </figure>

                                                        <h3 class="product-title">
                                                            <a href="/singleProduct/<%= product.productId._id %>">
                                                                <%= product.productId.name %>
                                                            </a>
                                                        </h3>
                                                    </div>
                                                </td>
                                                <td class="price-col">â‚¹<span id="price_<%= product.productId._id %>">
                                                    <% 
                                                        const pricePerUnit = product.productId.offerApplied ? 
                                                                            (product.productId.price - (product.productId.price * (product.productId.offerApplied / 100))) :
                                                                            product.productId.price;
                                                    %>
                                                    <%= pricePerUnit %>
                                                </span></td>
                                                <td class="quantity-col">
                                                    <div class="cart-product-quantity">
                                                        <input type="number" aria-readonly="false" class="form-control   quantity-input" 
                                                            id="quantity_<%= product.productId._id %>"
                                                            data-productid="<%= product.productId._id %>"
                                                            value="<%= product.quantity %>" min="1"
                                                            max="<%= product.productId.quantity %>" step="1"
                                                            data-decimals="0" onkeydown="return false" required
                                                            onchange="updateTotalPrice(this,'<%= product.productId._id %>')">
                                                    </div>
                                                </td>
                                                <td class="total-col">
                                                    <% const totalPrice = pricePerUnit * product.quantity; %>
                                                    â‚¹<span class="total-price" id="total_<%= product.productId._id %>">
                                                        <%= totalPrice %>
                                                    </span>
                                                </td>
                                                
                                                <td class="remove-col">
                                                    <button class="btn-remove"
                                                        onclick="removeFromCart('<%= product.productId._id %>','<%=  index%>')">
                                                        <i class="icon-close"></i></button>
                                                </td>
                                            </tr>
                                            <% }) %>
